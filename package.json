{
  "name": "stack-explorer",
  "description": "Blog of ZK-Jackie, built with Astro",
  "type": "module",
  "version": "1.0.0a1",
  "author": "ZK-Jackie",
  "license": "MIT",
  "keywords": ["astro", "blog", "tailwindcss", "typescript"],
  "repository": "https://github.com/ZK-Jackie/zk-jackie.github.io",
  "scripts": {
    "dev": "astro dev --mode development",
    "build:dev": "astro build --mode development",
    "build:dev:compressed": "astro build --mode development && node scripts/postbuild.js",
    "build:prod": "astro build --mode production && node scripts/postbuild.js",
    "build:cf": "astro build --mode cloudflare && node scripts/postbuild.js",
    "build:gh": "astro build --mode github && node scripts/postbuild.js",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "sync": "astro sync",
    "astro": "astro",
    "update-astro": "npx @astrojs/upgrade"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.6",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.6.0",
    "@iconify-json/mdi": "^1.2.3",
    "@tailwindcss/vite": "^4.1.7",
    "astro": "^5.13.10",
    "astro-expressive-code": "^0.41.3",
    "astro-icon": "^1.1.5",
    "fuse.js": "^7.1.0",
    "html-minifier-terser": "^7.2.0",
    "mdast-util-to-string": "^4.0.0",
    "picomatch": "^4.0.3",
    "playwright": "^1.55.0",
    "rehype-mermaid": "^3.0.0",
    "remark-gemoji": "^8.0.0",
    "tailwindcss": "^4.1.7",
    "terser": "^5.43.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/picomatch": "^4.0.2",
    "typescript": "^5.8.3"
  }
}
